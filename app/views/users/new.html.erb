<%= form_for(@user) do |f|%>
	 <%= render 'shared/error_messages', object: f.object %>

	 <div>
	 	<%= f.label :name%>
	 	<%= f.text_field :name%>
	 </div>

	 <div>
	 	<%= f.label :email %>
	 	<%= f.text_field :email %>
	 </div>

	 <div>
        <%= f.label :password %>
        <%= f.password_field :password %>
	 </div>

	 <div>
	 	<%= f.submit "Submit!!" %>
	 </div>

<% end %>